generator client {
  provider = "prisma-client-js"
  output   = "../generated/main"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL_PORTAL")
}

model News {
  id         Int      @id @default(autoincrement())
  title      String
  content    String
  image      String?
  created_at DateTime @default(now())

  @@map("news")
}
